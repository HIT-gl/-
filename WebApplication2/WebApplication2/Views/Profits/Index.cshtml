@model IEnumerable<WebApplication2.Models.Profit>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create") |
    @Html.ActionLink("Back", "Homeback")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SalesRevenue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SalesCost)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OperatingExpenses)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tax)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SalesRevenue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SalesCost)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OperatingExpenses)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tax)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id }) 
        </td>
    </tr>
}

</table>
